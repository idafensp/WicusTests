<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:wicus-hwspecs="http://purl.org/net/wicus-hwspecs#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:terms="http://purl.org/dc/terms/"
    xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" > 
  <rdf:Description rdf:about="http://purl.org/dc/terms/license">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-hwspecs#unit">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-hwspecs#Feature"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">whw:unit defines the unit of measurement of a whw:Feature</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-hwspecs">
    <terms:license rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://creativecommons.org/licenses/by-nc-sa/2.0/</terms:license>
    <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-06-14</terms:created>
    <owl:versionInfo>2.0</owl:versionInfo>
    <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://delicias.dia.fi.upm.es/~isantana/</dc:creator>
    <terms:description>The WICUS Hardware Specs ontology is describes the hardware characteristics of a computational infrastructure and the hardware requirements of a software component. This ontology is part of the WICUS Ontology network.</terms:description>
    <dc:title xml:lang="en">The WICUS Hardware Specs ontology</dc:title>
    <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-07-08</terms:modified>
    <vann:preferredNamespaceUri>http://purl.org/net/wicus-hwspecs#</vann:preferredNamespaceUri>
    <vann:preferredNamespacePrefix>whw</vann:preferredNamespacePrefix>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-hwspecs#hasHardwareComponent">
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-hwspecs#HardwareSpec"/>
    <rdfs:range rdf:resource="http://purl.org/net/wicus-hwspecs#HardwareComponent"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">whw:hasHardwareComponent links a whw:HardwareSpec to the set of whw:HardwareComponent that compose it.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-hwspecs#HardwareSpec">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A whw:HardwareSpec represents the set of hardware capabilities of a computational resource.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-hwspecs#hasFeature">
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-hwspecs#HardwareComponent"/>
    <rdfs:range rdf:resource="http://purl.org/net/wicus-hwspecs#Feature"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">whw:hasFeature links a whw:HardwareComponent to a whw:Feature defining its capabilities.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/created">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/creator">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-hwspecs#Network">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A whw:Network is a specific type of whw:HardwareComponent representing the Network type of a computational resource.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/wicus-hwspecs#HardwareComponent"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-hwspecs#Storage">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A whw:Storage is a specific type of whw:HardwareComponent representing the storage capacity of a computational resource.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/wicus-hwspecs#HardwareComponent"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Thing">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-hwspecs#Feature">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A whw:Feature represents a relevant characteristic of a component that define some of its capabilities.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-hwspecs#HardwareComponent">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A whw:HardwareComponent represents an element of the hardware stack of a computational resource.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-hwspecs#Memory">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A whw:Memory is a specific type of whw:HardwareComponent representing the RAM Memory of a computational resource.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/wicus-hwspecs#HardwareComponent"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-hwspecs#value">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-hwspecs#Feature"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">whw:unit defines the value of a whw:Feature</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-hwspecs#CPU">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A whw:CPU is a specific type of whw:HardwareComponent representing the CPU of a computational resource.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/wicus-hwspecs#HardwareComponent"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/modified">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/title">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/description">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
</rdf:RDF>
