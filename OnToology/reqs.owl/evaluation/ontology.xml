<rdf:RDF
    xmlns:p-plan="http://purl.org/net/p-plan#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:wicus-reqs="http://purl.org/net/wicus-reqs#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:terms="http://purl.org/dc/terms/"
    xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" > 
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#SoftwareRequirements">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A wreq:SoftwareRequirements is a specific type of wreq:ExecutionEnvironment, representing the software resources that must be provided to support the execution of a workflow.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/wicus-reqs#ExecutionEnvironment"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#AbstractWorkflow">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A wreq:AbstractWorkflow is a specific type of wreq:Workflow in which at least one of its steps is not fully specified.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/wicus-reqs#Workflow"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#HardwareRequirements">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A wreq:HardwareRequirements is a specific type of wreq:ExecutionEnvironment, representing the hardware resources that must be provided to support the execution of a workflow.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/wicus-reqs#ExecutionEnvironment"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#Workflow">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A wreq:Workflow is a specific type of p-plan:Plan, representing a computational scientific workflow from the point of view of its infrastructure.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/p-plan#Plan"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#RequirementLevel">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A wreq:RequirementLevel represents the degree of importance of a requirement.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#ExecutionEnvironment">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A wreq:ExecutionEnvironment represents the computational resources that must be provided to support the execution of a workflow.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/p-plan#isStepOfPlan">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#isStepOfWorkflow">
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-reqs#WorkflowStep"/>
    <rdfs:range rdf:resource="http://purl.org/net/wicus-reqs#Workflow"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/net/p-plan#isStepOfPlan"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wreq:isStepOfWorkflow is a specific type of p-plan:isStepOfPlan that relates a wreq:WorkflowStep to the wreqs:Workflow that it belongs to.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/modified">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/p-plan#Step">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/title">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/license">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/p-plan#Plan">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#WorkflowStep">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A wreq:WorkflowStep is a specific type of p-plan:Step, representing a single step of a workflow.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/p-plan#Step"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs">
    <terms:license rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://creativecommons.org/licenses/by-nc-sa/2.0/</terms:license>
    <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-07-08</terms:modified>
    <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-06-14</terms:created>
    <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://delicias.dia.fi.upm.es/~isantana/</dc:creator>
    <vann:preferredNamespacePrefix>wreq</vann:preferredNamespacePrefix>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <terms:description>The WICUS Workflow Execution Requirements ontology relates a Scientific Workflow to the execution requirements and dependencies for executing it. This ontology is part of the WICUS Ontology network.</terms:description>
    <dc:title xml:lang="en">The WICUS Workflow Execution Requirements ontology</dc:title>
    <owl:versionInfo>2.0</owl:versionInfo>
    <vann:preferredNamespaceUri>http://purl.org/net/wicus-reqs#</vann:preferredNamespaceUri>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#hasLevel">
    <rdfs:range rdf:resource="http://purl.org/net/wicus-reqs#RequirementLevel"/>
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-reqs#ExecutionEnvironment"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wreq:hasLevel links a wreq:ExecutionEnvironment to its corresponding wreq:RequirementLevel.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/created">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/creator">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#requiresExecutionEnvironment">
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-reqs#Workflow"/>
    <rdfs:range rdf:resource="http://purl.org/net/wicus-reqs#ExecutionEnvironment"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wreq:requiresExecutionEnvironment is a specific type of dcterms:requires that links a wreq:Worflow to the wreq:ExecutionEnvironment that must be available for executing it.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#hasConcreteWorkflow">
    <rdfs:range rdf:resource="http://purl.org/net/wicus-reqs#ConcreteWorkflow"/>
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-reqs#AbstractWorkflow"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wreq:hasConcreteWorkflow links an wreq:AbstractWorkflow to the set of wreq:ConcreteWorkflow that implements it.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Thing">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#ConcreteWorkflow">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A wreq:ConcreteWorkflow is a specific type of wreq:Workflow in which all its steps are fully specified.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/wicus-reqs#Workflow"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#hasSubworkflow">
    <rdfs:range rdf:resource="http://purl.org/net/wicus-reqs#Workflow"/>
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-reqs#Workflow"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wreq:hasSubworkflow links a wreq:Workflow to the subworkflows that compose it.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/description">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
</rdf:RDF>
