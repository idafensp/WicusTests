<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:wicus-sva="http://purl.org/net/wicus-sva#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:terms="http://purl.org/dc/terms/"
    xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" > 
  <rdf:Description rdf:about="http://purl.org/net/wicus-sva#providedBy">
    <rdfs:range rdf:resource="http://purl.org/net/wicus-sva#InfrastructureProvider"/>
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-sva#ImageAppliance"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wsva:providedBy links a wsva:ImageAppliance to its corresponding wsva:InfrastructureProvider</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/license">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-sva#InfrastructureProvider">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A wsva:InfrastructureProvider represents a service provider offering computational resources on demand.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-sva#ImageAppliance">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A wsva:ImageAppliance represents an image specification supporting the instantiation of an appliance in an infrastructure provider platform.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-sva#ScientificVirtualAppliance">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A wsva:ScientificVirtualAppliance represents a computational resource that may have been tuned for scientific purposes.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-sva#isSupportedBy">
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-sva#ScientificVirtualAppliance"/>
    <rdfs:range rdf:resource="http://purl.org/net/wicus-sva#ImageAppliance"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wsva:isSupportedBy links a wsva:ScientificVirtualAppliance to the set of wsva:ImageAppliance that supports it.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/created">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-sva">
    <owl:versionInfo>2.0</owl:versionInfo>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <terms:license rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://creativecommons.org/licenses/by-nc-sa/2.0/</terms:license>
    <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-07-08</terms:modified>
    <dc:title xml:lang="en">The WICUS Scientific Virtual Appliance ontology</dc:title>
    <terms:description>The WICUS Scientific Virtual Appliance ontology describes the virtual computational resources used for executing scientific experiments, usally as Virtual Machines deployed on a IaaS provider or a virtualization solution. This ontology is part of the WICUS Ontology network.</terms:description>
    <vann:preferredNamespacePrefix>wsva</vann:preferredNamespacePrefix>
    <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://delicias.dia.fi.upm.es/~isantana/</dc:creator>
    <vann:preferredNamespaceUri>http://purl.org/net/wicus-sva#</vann:preferredNamespaceUri>
    <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-06-14</terms:created>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-sva#VMImage">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A wsva:VMImage represents a Virtual Machine image that defines the basis of an appliance.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/creator">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-sva#vmId">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-sva#VMImage"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wsva:vmId defines the identifier of a wsva:VMImage</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Thing">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-sva#hasVMImage">
    <rdfs:range rdf:resource="http://purl.org/net/wicus-sva#VMImage"/>
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-sva#ImageAppliance"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wsva:hasVMImage links a wsva:ImageAppliance to the set of wsva:VMImage that supports it.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/modified">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/title">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/description">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
</rdf:RDF>
