<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:wreq="http://purl.org/net/wicus-reqs#"
    xmlns:wstack="http://purl.org/net/wicus-stack#"
    xmlns:wsva="http://purl.org/net/wicus-sva#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:terms="http://purl.org/dc/terms/"
    xmlns:whw="http://purl.org/net/wicus-hwspecs#"
    xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:wicus="http://purl.org/net/wicus#"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" > 
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#SoftwareRequirements">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-sva#ImageAppliance">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-stack#DeploymentStep">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-sva#ScientificVirtualAppliance">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#HardwareRequirements">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-stack#hasOutputInfo">
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-stack#DeploymentStep"/>
    <rdfs:range rdf:resource="http://purl.org/net/wicus-stack#ConfigurationInfo"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wstack:hasOutputInfo links a wstack:DeploymentStep to a wstack:ConfigurationInfo representing the output values resulting of executing a deployment step.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/requires">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus#composedBySoftwareStack">
    <rdfs:range rdf:resource="http://purl.org/net/wicus-stack#SoftwareStack"/>
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-reqs#SoftwareRequirements"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wicus:composedBySoftwareStack links a wreq:SoftwareRequirements to a wstack:SoftwareStack defining the required software characteristics.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus#composedByHardwareSpec">
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-reqs#HardwareRequirements"/>
    <rdfs:range rdf:resource="http://purl.org/net/wicus-hwspecs#HardwareSpec"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wicus:composedByHardwareSpec links a wreq:HardwareRequirements to a whw:HardwareSpec defining the required hardware characteristics.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/modified">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/title">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/license">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus#requiresHardware">
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-stack#SoftwareComponent"/>
    <rdfs:range rdf:resource="http://purl.org/net/wicus-hwspecs#HardwareSpec"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/requires"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wicus:requiresHardware links a wstack:SoftwareComponent to a whw:HardwareSpec that defines the necessary hardware characteristics for deploying it.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-stack#Version">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-stack#SoftwareStack">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus">
    <owl:imports rdf:resource="http://purl.org/net/wicus-stack"/>
    <owl:imports rdf:resource="http://purl.org/net/wicus-hwspecs"/>
    <owl:imports rdf:resource="http://purl.org/net/wicus-sva"/>
    <terms:description>The WICUS ontology network is a set of ontologies designed to represent the information about the computational infrastructure involved on the execution of computational scientific experiments modelled as scientific workflows.</terms:description>
    <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-06-14</terms:created>
    <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-07-08</terms:modified>
    <terms:license rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://creativecommons.org/licenses/by-nc-sa/2.0/</terms:license>
    <owl:imports rdf:resource="http://purl.org/net/wicus-reqs"/>
    <vann:preferredNamespacePrefix>wicus</vann:preferredNamespacePrefix>
    <dc:title>The Workflow Infrastructure Conservation Using Semantics ontology</dc:title>
    <owl:versionInfo>2.0</owl:versionInfo>
    <vann:preferredNamespaceUri>http://purl.org/net/wicus#</vann:preferredNamespaceUri>
    <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://delicias.dia.fi.upm.es/~isantana/</dc:creator>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-hwspecs#HardwareSpec">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-stack#ConfigurationInfo">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/created">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/creator">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-reqs#requiresExecutionEnvironment">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/requires"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus#hasSoftwareStack">
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-sva#ScientificVirtualAppliance"/>
    <rdfs:range rdf:resource="http://purl.org/net/wicus-stack#SoftwareStack"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wicus:hasSoftwareStack links a wsva:ScientificVirtualAppliance to a wstack:SoftwareStack defining the software characteristics of a scientific appliance.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus#hasHardwareSpecs">
    <rdfs:domain rdf:resource="http://purl.org/net/wicus-sva#ImageAppliance"/>
    <rdfs:range rdf:resource="http://purl.org/net/wicus-hwspecs#HardwareSpec"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">wicus:hasHardwareSpecs links a wsva:ImageAppliance to a whw:HardwareSpec that defines the image hardware capabilities.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/wicus-stack#SoftwareComponent">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/description">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
